{
  "name": "vidhitraders",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "patch_mac": "cd patches && ./PatchFile.sh && cd ..",
    "patch_win": "cd patches && ./PatchFileWin.bat && cd ..",
    "android": "cd patches && ./PatchFile.sh && cd .. && react-native run-android",
    "android:w": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "build_clean": "watchman watch-del-all && cd android && ./gradlew clean && cd ..",
    "build": "watchman watch-del-all && cd android && ./gradlew assembleRelease && cd .. && open ./android/app/build/outputs/apk/release",
    "build:win": "cd android && ./gradlew assembleRelease && cd .. && explorer .\\android\\app\\build\\outputs\\apk\\release",
    "build_B": "watchman watch-del-all && cd android && ./gradlew bundleRelease && cd .. && open ./android/app/build/outputs/bundle/release",
    "android_R": "react-native run-android --variant=release",
    "pod": "cd ios && pod install --repo-update && cd .."
  },
